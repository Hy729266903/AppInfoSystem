<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.appsys.dao.deve.datadictionary.DeveDictionaryMapper">
	<!-- 查询app状态 -->
	<select id="seleName" resultType="DataDictionary">
		SELECT * FROM data_dictionary WHERE `typeCode`='APP_STATUS'
	</select>
	
	<!-- 所属平台 -->
	<select id="selePlatform" resultType="DataDictionary">
		SELECT * FROM data_dictionary WHERE `typeCode`='APP_FLATFORM'
	</select>
</mapper>